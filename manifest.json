{
  "manifest_version": 3,
  "name": "Wistant Feedback TOC",
  "version": "0.2.0",
  "permissions": ["activeTab", "scripting"],
  "action": { "default_popup": "popup.html" },
  "content_scripts": [
    {
      "js": ["src/content.tsx"],
      "matches": ["https://*.wistant.com/feedbacks/*/sheets/*"]
    }
  ]
}
